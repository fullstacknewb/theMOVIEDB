@page 
@model ActorModel 

@if(Model.fetch.actor != null){
    <h1>@Model.fetch.actor.biography</h1>
    <p>@Model.fetch.actor.place_of_birth</p>
    <p>@Model.fetch.actor.birthday</p>
}

@if(Model.fetch.profileSet != null)
{
    @for(int i = 0; i < Model.fetch.profileSet.profiles.Count; i++) {
               string path = "https://image.tmdb.org/t/p/w500/" + Model.fetch.profileSet.profiles.ElementAt(i).file_path; 
              <img src="@path" alt="pic">
}
}
